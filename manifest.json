{
    "name": "OpenAI Codefixer",
    "version": "1.0.0",
    "manifest_version": 3,
    "author": "Kevin Fritsch",
    "permissions": ["storage"],
    "action": {
        "default_popup": "src/html/popup.html",
        "default_icon": {
            "16": "src/img/openai16.png",
            "32": "src/img/openai32.png",
            "48": "src/img/openai48.png",
            "128": "src/img/openai128.png"
        }
    },
    "icons": {
        "16": "src/img/openai16.png",
        "32": "src/img/openai32.png",
        "48": "src/img/openai48.png",
        "128": "src/img/openai128.png"
    },
    "options_page": "src/html/options.html"
}